{% extends "base.html.twig" %}
{% block title %}
    Edit category {{ category.name }}
{% endblock %}
{% block body %}
   <h1>Edit category {{ category.name }}</h1>
    {{ form_start(formView) }}
    {{ form_errors(formView) }}
    <div class="row">
        <div class="col">
            {{ form_row(formView.name) }}
        </div>
    </div>

    <button class="submit btn btn-primary">
        <i class="fas fa-save">
            Add product
        </i>
    </button>
{% endblock %}

